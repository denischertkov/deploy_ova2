#cloud-config
runcmd:
  - date > /tmp/cloud-config.txt
#  - echo nameserver 9.9.9.9  > /etc/resolv.conf
#ssh-rsa 

#cloud-init
bootcmd:
  - echo "Hello, world!" > /tmp/cidata.txt
  - mount /dev/sr0 /mnt && cp /mnt/network.conf /etc/netplan/00-installer-config.yaml
  - netplan apply
  - date >> /tmp/cidata.txt
